/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=nandab);
    Nand(a=a, b=nandab, out=anandb);
    Nand(a=nandab, b=b, out=nandbb);
    Nand(a=anandb, b=nandbb, out=out);
}